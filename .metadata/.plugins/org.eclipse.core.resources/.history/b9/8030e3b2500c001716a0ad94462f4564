		int[] dad = new int[g.vertices + 1];
		int[] fringe = new int[g.vertices + 1];
		Arrays.fill(bandwidth, 101); //the maximum weight i set is 100
		Arrays.fill(dad, -1);
		//initial the source and fringe
		return dad;
	}
