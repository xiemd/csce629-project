
public class Edge {
	private int start;
	private int end;
	private double weight;
	
	public Edge(int start, int end){
		if (start < end){
			this.start = start;
			this.end = end;
		}else{
			this.start = end;
			this.end = start;
		}
	}
	public void addWeight(double weight){
		this.weight = weight;
	}
	public String toString(){
		return start + "-" + end;
	}
//	public static void main(String[] args){
//		Edge a = new Edge(2, 1);
//		System.out.println(a);
//				
//	}
}


