public class maxHeap {
	private int[] H; //record the vertices
	private int[] D; // record the vertices' value
	
	public maxHeap(graph g) {
		H = new int[g.vertices + 1];
		D = new int[g.vertices + 1];
		for (int i : H){ H[i] = i; }
		for (int j : D){ D[j] = 0; }
		
		
		
	}
