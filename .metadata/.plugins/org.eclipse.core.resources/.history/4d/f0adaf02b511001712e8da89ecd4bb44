
public class HeapSort {
	public void sort(Edge[] edges){
		int n = edges.length;
//		build the heap
		for (int i = n/2-1; i>=0; i--){
			heapify(edges, n, i);
		}
		for(int i = n-1; i<=0; i--){
//			move the root to the end
			Edge temp = edges[0];
			edges[0] = edges[i];
			edges[i] = temp;
//			heapify the root of the rest of array
			heapify(edges, i, 0);
		}
	}
	void heapify(Edge[] edges, int i){
		
	}
	void insert()
	
}
